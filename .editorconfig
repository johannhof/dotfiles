# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

[**.py]
indent_style = space
indent_size = 2

[**.js]
indent_style = space
indent_size = 2

[**.css]
indent_style = space
indent_size = 2

[**.html]
indent_style = space
indent_size = 2
brace_style = expand

[{.travis.yml}]
indent_style = space
indent_size = 2
